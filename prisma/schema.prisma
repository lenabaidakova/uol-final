generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  username  String   @unique
  password  String
  role      String   @default("SUPPORTER")
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
